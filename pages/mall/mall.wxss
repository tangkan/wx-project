.weui-search-bar__cancel-btn {
  font-size: 16px;
}

.page__bd {
  margin-bottom: 15px;
}

.all {
  font-size: 16px;
  margin-left: 20px;
  
  color: #b4b3b3;
}

.list-nav {
  margin-top: 15px;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.list {
  width: 33%;
  height: 100px;
  margin-bottom: 10px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.list > view {
  width: 64px;
  height: 64px;
  background-color: #dedede;
  border-radius: 50%;
  overflow: hidden;
}

.list > view image {
  width: 100%;
  height: 100%;
}

.list >text {
  font-size: 14px;
  margin-top: 5px;
}